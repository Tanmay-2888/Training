<h2>New Product</h2>
<div class="container">
@{Html.BeginForm();}
<div class="row">
    <div class="col-lg-3">
        @Html.Label("Product Name")


    </div>
    <div class="col-lg-3">
        @Html.TextBox("Name")


    </div>

</div>
<div class="row">
    <div class="col-lg-3">
        @Html.Label("Product Price")


    </div>
    <div class="col-lg-3">
        @Html.TextBox("Price")


    </div>

</div>
<input type="submit" value="Create" />

</div>
